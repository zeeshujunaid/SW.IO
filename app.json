{
  "expo": {
    "name": "SW.IO",
    "slug": "SW.IO",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/saylanilogo.png",  // App icon
    "scheme": "swio",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "icon": "./assets/images/saylanilogo.png"  // optional, iOS specific
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/saylanilogo.png",  // main logo
        "backgroundColor": "#E6F4FE"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/saylanilogo.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/saylanilogo.png", // extension required
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
